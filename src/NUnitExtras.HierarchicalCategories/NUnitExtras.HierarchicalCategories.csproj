<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net40;net45;netstandard2.0</TargetFrameworks>
    <RootNamespace>NUnit.Extras</RootNamespace>
    <Version>1.4.0</Version>
    <Product>NUnit Extras</Product>
    <Description>Hierarchical categories for NUnit.

